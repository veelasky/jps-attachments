{
  "name": "jalameta/jps-attachments",
  "description": "Easy attachment library to attach your media files into something attachable.",
  "type": "laravel-plugin",
  "require": {
    "php": "^7.2 || ^8.0",
    "illuminate/database": ">=6.18",
    "illuminate/support": ">=6.18",
    "illuminate/console": ">=6.18",
    "illuminate/filesystem": ">=6.18"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.5",
    "orchestra/testbench": ">=4.0"
  },
  "suggest": {
    "moontoast/math": "^1.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rifki Alhuraibi",
      "email": "veelasky@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jalameta\\Attachments\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Jalameta\\Attachments\\AttachmentServiceProvider"
      ]
    }
  }
}
